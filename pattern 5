#include <bits/stdc++.h>
using namespace std;
class solution{
public:
void printsquare(int n){
         for(int i=n;i>=1;i--){
        for(int j=1;j<=i;j++){
            cout<<"* ";
        }
        cout<<endl;
    }
    }
};
//Driver Code Starts
int main(){
int t;
cin>>t;
while(t--){
int n;
cin>>n;
solution ob;
ob.printsquare(n);
}
return 0;
}
//Driver Code Ends
